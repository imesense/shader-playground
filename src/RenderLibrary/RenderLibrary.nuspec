<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
    <metadata>
        <id>ImeSense.ShaderPlayground.Render</id>
        <version>0.1.0</version>
        <title>ImeSense.ShaderPlayground.Render</title>
        <authors>acidicMercury8</authors>
        <requireLicenseAcceptance>true</requireLicenseAcceptance>
        <license type="file">LICENSE</license>
        <readme>README.md</readme>
        <projectUrl>https://github.com/imesense/shader-playground</projectUrl>
        <repository type="git" url="https://github.com/imesense/shader-playground" />
        <description>Render library</description>
        <releaseNotes></releaseNotes>
        <copyright>&#169; 2023 ImeSense</copyright>
        <tags>Native native nativepackage</tags>

        <dependencies>
            <group targetFramework="net6.0" />
            <group targetFramework="net7.0" />
            <group targetFramework="net8.0" />
        </dependencies>
    </metadata>

    <files>
        <file src="..\..\README.md" target="README.md" />
        <file src="..\..\LICENSE" target="LICENSE" />

        <file src="..\..\bin\Win32\Debug\*.dll" target="runtimes\win-x86\native\Debug\" />
        <file src="..\..\bin\Win32\Debug\*.pdb" target="runtimes\win-x86\native\Debug\" />
        <file src="..\..\bin\Win32\Release\*.dll" target="runtimes\win-x86\native\Release\" />
        <file src="..\..\bin\Win32\Release\*.pdb" target="runtimes\win-x86\native\Release\" />

        <file src="..\..\bin\x64\Debug\*.dll" target="runtimes\win-x64\native\Debug\" />
        <file src="..\..\bin\x64\Debug\*.pdb" target="runtimes\win-x64\native\Debug\" />
        <file src="..\..\bin\x64\Release\*.dll" target="runtimes\win-x64\native\Release\" />
        <file src="..\..\bin\x64\Release\*.pdb" target="runtimes\win-x64\native\Release\" />

        <file src="..\..\bin\Win32\Debug\*.lib" target="lib\win-x86\native\Debug\" />
        <file src="..\..\bin\Win32\Debug\*.exp" target="lib\win-x86\native\Debug\" />
        <file src="..\..\bin\Win32\Release\*.lib" target="lib\win-x86\native\Release\" />
        <file src="..\..\bin\Win32\Release\*.exp" target="lib\win-x86\native\Release\" />

        <file src="..\..\bin\x64\Debug\*.lib" target="lib\win-x64\native\Debug\" />
        <file src="..\..\bin\x64\Debug\*.exp" target="lib\win-x64\native\Debug\" />
        <file src="..\..\bin\x64\Release\*.lib" target="lib\win-x64\native\Release\" />
        <file src="..\..\bin\x64\Release\*.exp" target="lib\win-x64\native\Release\" />

        <file src="*.jpg" target="res\" />
        <file src="*.vs" target="res\" />
        <file src="*.ps" target="res\" />

        <file src="*.hpp" target="include\" />

        <file src="RenderLibrary.Native.targets" target="build\native\ImeSense.ShaderPlayground.Render.targets" />
        <file src="RenderLibrary.Net.targets" target="build\net6.0\ImeSense.ShaderPlayground.Render.targets" />
        <file src="RenderLibrary.Net.targets" target="build\net7.0\ImeSense.ShaderPlayground.Render.targets" />
        <file src="RenderLibrary.Net.targets" target="build\net8.0\ImeSense.ShaderPlayground.Render.targets" />
    </files>
</package>
